<Window x:Class="PressureGaugeCodeGenerator.Windows.ListNumbersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Номера в файле" Height="287.282" Width="226.476" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="logo_small.ico" Loaded="ListNumbersWindow_OnLoaded">
    <Grid>
        <TextBox x:Name="TextBoxNumbers" 
                 Margin="10,32,10,44" 
                 TextWrapping="Wrap" 
                 IsReadOnly="True"
                 HorizontalContentAlignment="Left"
                 ScrollViewer.VerticalScrollBarVisibility="Auto"
                 Style="{StaticResource textBoxs}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        <TextBlock Margin="10,10,10,0" 
                   Text="Список номеров в файле:" 
                   Style="{StaticResource textBlocks}" 
                   HorizontalAlignment="Stretch"/>
        <Button Content="Окей" 
                x:Name="ButtonOK"             
                Width="107"                
                Margin="56,0,0,7"
                Style="{StaticResource buttons}"
                Click="ButtonOK_OnClick"/>
    </Grid>
</Window>
